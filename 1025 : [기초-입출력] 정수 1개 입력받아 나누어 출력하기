#include <stdio.h>

int main()
{
	int num = 0;
	
	scanf("%d", &num);

	printf("[%d0000]\n", num / 10000);
	printf("[%d000]\n", (num % 10000)/1000);
	printf("[%d00]\n", ((num % 10000) % 1000)/100);
	printf("[%d0]\n", (((num % 10000) % 1000) % 100)/10);
	printf("[%d]\n", (((num % 10000) % 1000) % 100) % 10);

	return 0;
}

//////////

#include <stdio.h>
int main()
{
    int a, b, c, d, e;
    scanf("%1d%1d%1d%1d%1d", &a, &b, &c, &d, &e);
    printf("[%d]\n", a*10000);
    printf("[%d]\n", b*1000);
    printf("[%d]\n", c*100);
    printf("[%d]\n", d*10);
    printf("[%d]\n", e);
    return 0;
}
